#time limit exceeded

class Solution:
    def maxSubArray(self, nums: List[int]) -> int:
        maxy_arr  = []
        for i in range(len(nums)):
            for j in range(i+1, len(nums)+1):
                sub_array = nums[i:j]
                adds = sum(sub_array)
                maxy_arr.append(adds)
        return max(maxy_arr)
